# Malta Parking Finder - Environment Configuration

# ============================================
# Database Configuration
# ============================================
VITE_DB_HOST=localhost
VITE_DB_PORT=3306
VITE_DB_USER=root
VITE_DB_PASSWORD=MySecurePassword123
VITE_DB_NAME=malta_parking

# ============================================
# Backend API Configuration
# ============================================
VITE_API_URL=http://localhost:5000
VITE_API_TIMEOUT=30000

# ============================================
# Authentication Configuration
# ============================================
VITE_JWT_SECRET=your_super_secret_jwt_key_change_this_in_production
VITE_JWT_EXPIRY=24h

# ============================================
# Frontend Configuration
# ============================================
VITE_APP_NAME=Malta Parking Finder
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# ============================================
# Map Configuration
# ============================================
VITE_MAP_CENTER_LAT=35.8992
VITE_MAP_CENTER_LON=14.4417
VITE_MAP_ZOOM=11

# ============================================
# Feature Flags
# ============================================
VITE_ENABLE_REAL_TIME_UPDATES=true
VITE_ENABLE_USER_FAVORITES=true
VITE_ENABLE_ADMIN_PANEL=true

# ============================================
# Logging Configuration
# ============================================
VITE_LOG_LEVEL=info
VITE_ENABLE_ANALYTICS=false

# ============================================
# File Upload Configuration
# ============================================
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_IMAGE_FORMATS=jpg,jpeg,png,gif

# ============================================
# Testing Configuration
# ============================================
VITE_TEST_MODE=false

